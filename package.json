{
  "name": "com.davidhellmann.sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run codegen --watch\" \"vite dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env",
    "icons:add": "npx @sly-cli/sly add --overwrite",
    "a": "sh ./scripts/scopes-all.sh",
    "f": "sh ./scripts/scopes-focus.sh"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.6",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "dayjs": "^1.11.13",
    "embla-carousel": "^8.6.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "shiki": "^3.2.2",
    "svelte": "5.22.4",
    "svelte-check": "^4.1.6",
    "svelte-time": "^2.0.1",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.2.6",
    "vite-svg-sprite-wrapper": "^1.4.1",
    "vitest": "^3.1.1"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/bitter": "^5.2.6",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/jetbrains-mono": "^5.2.5",
    "@fontsource/poppins": "^5.2.5",
    "@parcel/watcher": "^2.5.1",
    "@sly-cli/sly": "^1.14.0",
    "@tailwindcss/vite": "^4.0.17",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-svelte": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.0.2",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
    "lightgallery": "^2.8.3",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^7.0.1",
    "svgo": "^3.3.2",
    "tailwind-variants": "^0.3.1",
    "unlazy": "^0.12.4"
  }
}
