{
  "name": "com.davidhellmann.sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "svelte-kit sync && concurrently \"npm run codegen --watch\" \"vite dev\"",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env",
    "icons:add": "npx @sly-cli/sly add --overwrite",
    "a": "sh ./scripts/scopes-all.sh",
    "f": "sh ./scripts/scopes-focus.sh"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.4.0",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "embla-carousel": "^8.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.14.0",
    "globals": "^17.2.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "shiki": "^3.21.0",
    "svelte": "5.49.0",
    "svelte-check": "^4.3.5",
    "svelte-time": "^2.1.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-svg-sprite-wrapper": "^1.5.0",
    "vitest": "^4.0.18"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/bitter": "^5.2.10",
    "@fontsource-variable/inter": "^5.2.8",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@fontsource/poppins": "^5.2.7",
    "@parcel/watcher": "^2.5.6",
    "@sly-cli/sly": "^2.1.1",
    "@tailwindcss/vite": "^4.1.18",
    "clsx": "^2.1.1",
    "embla-carousel-auto-scroll": "^8.6.0",
    "embla-carousel-svelte": "^8.6.0",
    "embla-carousel-wheel-gestures": "^8.1.0",
    "graphql-request": "^7.4.0",
    "graphql-tag": "^2.12.6",
    "lightgallery": "2.9.0",
    "lodash-es": "^4.17.23",
    "node-html-parser": "^7.0.2",
    "svgo": "^4.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "unlazy": "^1.1.0"
  }
}
