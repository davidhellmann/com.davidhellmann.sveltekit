{
  "name": "com.davidhellmann.sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run codegen --watch\" \"vite dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env",
    "icons:add": "npx @sly-cli/sly add --overwrite"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.13.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "shiki": "^1.24.3",
    "svelte": "5.2.12",
    "svelte-check": "^4.1.1",
    "svelte-time": "^0.9.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.9",
    "vite-svg-sprite-wrapper": "^1.4.1",
    "vitest": "^2.1.8"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/bitter": "^5.1.0",
    "@fontsource-variable/inter": "^5.1.0",
    "@fontsource-variable/jetbrains-mono": "^5.1.1",
    "@fontsource/poppins": "^5.1.0",
    "@parcel/watcher": "^2.5.0",
    "@sly-cli/sly": "^1.14.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "embla-carousel-svelte": "^8.5.1",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
    "lightgallery": "^2.8.2",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^6.1.13",
    "svgo": "^3.3.2",
    "tailwind-variants": "^0.3.0",
    "tailwindcss-fluid-type": "^2.0.7",
    "unlazy": "^0.12.1"
  }
}
