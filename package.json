{
  "name": "com.davidhellmann.sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run codegen --watch\" \"vite dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --require dotenv/config --config codegen.ts dotenv_config_path=.env",
    "icons:add": "npx @sly-cli/sly add"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.1",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.3.1",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.8.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.8",
    "shiki": "^1.22.2",
    "svelte": "5.2.0",
    "svelte-check": "^4.0.7",
    "svelte-time": "^0.9.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11",
    "vite-svg-sprite-wrapper": "^1.3.3",
    "vitest": "^2.1.5"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/bitter": "^5.1.0",
    "@fontsource-variable/inter": "^5.1.0",
    "@fontsource-variable/jetbrains-mono": "^5.1.1",
    "@parcel/watcher": "^2.5.0",
    "@sly-cli/sly": "^1.14.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "embla-carousel-svelte": "^8.3.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "lightgallery": "^2.8.1",
    "lodash-es": "^4.17.21",
    "node-html-parser": "^6.1.13",
    "tailwind-variants": "^0.2.1",
    "tailwindcss-fluid-type": "^2.0.6",
    "unlazy": "^0.11.8"
  }
}
